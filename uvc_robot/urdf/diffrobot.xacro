<robot name="diffrobot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find uvc_robot)/urdf/diffrobot.gazebo"/>

<link name="robot_footprint">  
</link>

<joint name="robot_footprint_joint" type="fixed">
    <parent link ="robot_footprint"/>
    <child link ="base_link"/>   
</joint>


<link name="base_link">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="10.0"/>
        <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual name="base_link_visual">
        <geometry>
           <cylinder radius="0.5" length="0.4"/>
        </geometry>
        <material name="">
            <color rgba="1.0 0.0 0.0 1.0"/>
            <texture filename=""/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
           <cylinder radius="0.5" length="0.45"/>
        </geometry>
    </collision>
</link>

<link name="cylinder_link">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="1.0"/>
        <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual name="cylinder_link_visual">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.06" length="0.5"/>
        </geometry>
        <material name="">
            <color rgba="0.0 1.0 0.0 1.0"/>
            <texture filename=""/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
           <cylinder radius="0.06" length="0.5"/>
        </geometry>
    </collision>
</link>

<joint name="cylinder_joint" type="fixed">
    <origin xyz="0.0 0.0 0.36" rpy="0.0 0.0 0.0"/>
    <parent link="base_link"/>
    <child link="cylinder_link"/>
    <axis xyz="0.0 0.0 0.0"/>
    <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
</joint>



<link name="left_wheel">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="3.0"/>
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
    <visual name="left_wheel_visual">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.3" length="0.05"/>
        </geometry>
        <material name="">
            <color rgba="0.0 0.0 1.0 1.0"/>
            <texture filename=""/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.3" length="0.05"/>
        </geometry>
    </collision>
</link>


<joint name="left_wheel_joint" type="continuous">
    <origin xyz="0.0 0.55 0.0" rpy="0.0 1.5707 1.5707"/>
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
</joint>


<link name="right_wheel">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="3.0"/>
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
    <visual name="right_wheel_visual">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
          <cylinder radius="0.3" length="0.05"/>
        </geometry>
        <material name="">
            <color rgba="0.0 0.0 1.0 1.0"/>
            <texture filename=""/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
           <cylinder radius="0.3" length="0.05"/>
        </geometry>
    </collision>
</link>


<joint name="right_wheel_joint" type="continuous">
    <origin xyz="0.0 -0.55 0.0" rpy="0.0 1.5707 1.5707"/>
    <parent link="base_link"/>
    <child link="right_wheel"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
</joint>

<link name="front_caster">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.0"/>
        <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual name="front_caster_visual">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
             <sphere radius="0.1"/>
        </geometry>
        <material name="">
            <color rgba="1.0 1.0 0.0 1.0"/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
             <sphere radius="0.1"/>
        </geometry>
    </collision>
</link>

<joint name="front_caster_joint" type="fixed">
    <origin xyz="0.35 0.0 -0.197999" rpy="0.0 1.5707 1.5707"/>
    <parent link="base_link"/>
    <child link="front_caster"/>
    <axis xyz="0.0 0.0 0.0"/>
    <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
</joint>


<link name="back_caster">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.0"/>
        <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual name="back_caster_visual">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
             <sphere radius="0.1"/>
        </geometry>
        <material name="">
            <color rgba="1.0 1.0 0.0 1.0"/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
             <sphere radius="0.1"/>
        </geometry>
    </collision>
</link>

<joint name="back_caster_joint" type="fixed">
    <origin xyz="-0.35 0.0 -0.197999" rpy="0.0 1.5707 1.5707"/>
    <parent link="base_link"/>
    <child link="back_caster"/>
    <axis xyz="0.0 0.0 0.0"/>
    <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
</joint>


<link name="lidar_link">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.1"/>
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
    <visual name="lidar_link_visual">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
       <mesh filename="package://uvc_robot/meshes/lidar_link.STL"/>     
          </geometry>
        <material name="">
            <color rgba="0.0 1.0 0.0 1.0"/>
            <texture filename=""/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <box size="0.1 0.1 0.1"/>
        </geometry>
    </collision>
</link>

<joint name="lidar_joint" type="fixed">
 <origin xyz="0.45 0.0 0.235" rpy="0.0 0.0 0.0"/>
  <parent link="base_link"/>
    <child link="lidar_link"/>
    <axis xyz="0.0 0.0 0.0"/>
    <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
</joint>

    <link name="laser_data_link">
        
    </link>

    <joint name="laser_data_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 -1.5708 -1.5708"/>
        <parent link="lidar_link"/>
        <child link="laser_data_link"/>
    </joint>

 

  <!-- Camera -->
  <link name="camera_link">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.1"/>
        <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual name="camera_link_visual">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
         <mesh filename="package://uvc_robot/meshes/kinect.dae"/>
          </geometry>
        <material name="">
            <color rgba="0.0 1.0 0.0 1.0"/>
            
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <box size="0.1 0.1 0.1"/>
        </geometry>
    </collision>
</link>
<joint name="camera_joint" type="fixed">
 <origin xyz="0 0.0 0.28" rpy="0 0 0"/>
  <parent link="cylinder_link"/>
    <child link="camera_link"/>
    <axis xyz="0.0 0.0 0.0"/>
    <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
</joint>
<link name="camera_link_optical"></link>
  <joint name="camera_optical_joint" type="fixed">
        <parent link="camera_link"/>
        <child link="camera_link_optical"/>
        <origin xyz="0 0 0" rpy="0.0 -1.5708 -1.5708"/>
    </joint>

   

</robot>